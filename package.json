{
  "name": "hanna",
  "version": "1.5.3",
  "description": "Hyllest til Hanna",
  "private": true,
  "main": "index.js",
  "scripts": {
    "test": "standard && npm audit && ava",
    "test-offline": "standard && ava",
    "coverage": "nyc ava",
    "coveralls": "nyc ava && nyc report --reporter=lcov && cat coverage/lcov.info | coveralls",
    "standard-fix": "standard --fix",
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "now-build": "next build",
    "deploy": "npm test && now && now alias",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm install"
  },
  "author": "Geir GÃ¥sodden <geir.gasodden@pythonia.no> (https://github.com/zrrrzzt)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zrrrzzt/hanna.git"
  },
  "bugs": {
    "url": "https://github.com/zrrrzzt/hanna/issues"
  },
  "homepage": "https://github.com/zrrrzzt/hanna#readme",
  "dependencies": {
    "dom-to-image": "2.6.0",
    "file-saver": "2.0.2",
    "next": "9.1.1",
    "next-offline": "4.0.6",
    "random-angry-emoji": "1.0.3",
    "random-shout": "1.1.4",
    "react": "16.10.2",
    "react-dom": "16.10.2"
  },
  "devDependencies": {
    "ava": "2.4.0",
    "babel-eslint": "10.0.3",
    "coveralls": "3.0.7",
    "nyc": "14.1.1",
    "standard": "14.3.1"
  },
  "standard": {
    "ignore": [
      "/static/*.js"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true,
        "jsx": true
      },
      "sourceType": "module"
    }
  },
  "engines": {
    "node": ">= 10.16.3"
  }
}
